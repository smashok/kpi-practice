<Window x:Class="Lab3_1.View.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab3_1.View"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="label" Content="Username:" HorizontalAlignment="Left" Margin="286,123,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxUsername" HorizontalAlignment="Left" Height="23" Margin="286,154,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="211"/>
        <Label x:Name="label_Copy" Content="Password:" HorizontalAlignment="Left" Margin="286,182,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxPassword" HorizontalAlignment="Left" Height="23" Margin="286,213,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="211"/>
        <Button x:Name="buttonLogin" Content="Log in" HorizontalAlignment="Left" Margin="286,241,0,0" VerticalAlignment="Top" Width="211" Click="ButtonLogin_Click"/>

    </Grid>
</Window>
